{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "dependsOn": ["^build", "^generate"],
            "outputs": [".next/**", "!.next/cache/**", "dist/**"]
        },
        "lint": {},
        "dev": {
            "cache": false,
            "persistent": true
        },
        "generate": {
            "dependsOn": ["^generate"],
            "cache": false
        },
        "clean": {
            "cache": false
        }
    }
}
